        "targets": [
            "last 2 versions",
            "not dead",
            "not < 2%"
                ],
        "corejs": "2",
        "useBuiltIns": "entry"
            }
        ], "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "react-hot-loader/babel"
    ]
}